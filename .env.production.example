# Production Environment Variables (Example)
# 실제 운영 시 .env.production으로 복사하여 사용

NEXT_PUBLIC_STAGE=production
STAGE=production

# Database (설정 필수)
DB_TYPE=postgresql
DB_HOST=prod-db.example.com
DB_PORT=5432
DB_NAME=aiproject_prod
DB_USER=
DB_PASSWORD=
DB_SSL=required

# Security (설정 필수)
JWT_SECRET=
API_KEY=

# Logging
LOG_LEVEL=warn

# Features
AUTH_BYPASS=false
DEBUG_PANEL=false
MAINTENANCE_MODE=false
